{
    "workbench.activityBar.visible": true,
    "editor.fontSize": 15,
    "explorer.confirmDelete": false,
    "terminal.integrated.inheritEnv": true,
    //"terminal.integrated.shell.linux": "/bin/bash",
    "python.terminal.activateEnvironment": false,
    "workbench.iconTheme": "material-icon-theme",
    //"python.pythonPath": "/usr/bin/python3",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.activityBar.iconClickBehavior": "focus",
    "workbench.editor.enablePreview": false,
    "terminal.integrated.automationShell.linux": "",
    "python.linting.flake8Path": "/home/rishi/.local/bin/flake8",
    "python.linting.pycodestyleEnabled": false,
    "bracketPairColorizer.excludedLanguages": [

    ],
    "workbench.sideBar.location": "left",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.colorCustomizations": {
        "editorWhitespace.foreground": "#ff000d",
    },
    "[git-commit]": {

        "editor.rulers": [
            120
        ],
        "workbench.editor.restoreViewState": false
    },
    "editor.rulers": [
        120
    ],
    "editor.fontFamily": "'DejaVu Sans Mono'",
    "editor.minimap.enabled": false,
    "window.zoomLevel": -1,
    //"editor.codeActionsOnSave": null,
    "editor.renderWhitespace": "trailing",
    "breadcrumbs.enabled": true,
    "python.analysis.diagnosticSeverityOverrides": {
            "reportUnboundVariable" : "information",
            "reportImplicitStringConcatenation" : "warning",
            "reportMissingImports":"none",
            "reportMissingModuleSource": "none",
    },
    "python.defaultInterpreterPath": "/usr/bin/python3",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Args": [
        "--extend-ignore=E203,E266,E501,W503",
        "--max-line-length=120",
        "--select=B,C,E,F,W,T4,B9",
        "--max-complexity=18"
    ],
    "python.linting.flake8CategorySeverity.W": "Error",
    "workbench.colorTheme": "Visual Studio Dark",
    "editor.tokenColorCustomizations": null,
    "workbench.editor.untitled.hint": "hidden",
    "python.linting.pylintEnabled": false,
    "python.linting.enabled": true,
    //"terminal.integrated.localEchoStyle": null
}